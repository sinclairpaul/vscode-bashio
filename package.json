{
	"name": "completions-bashio",
	"displayName": "Completion Item Provider for Bashio",
	"version": "0.0.1",
	"publisher": "sinclairpaul-bashio",
	"repository": {
		"type": "git",
		"url": "https://github.com/sinclairpaul/vscode-bashio"
	},
	"bugs": {
		"url": "https://github.com/sinclairpaul/vscode-bashio/issues"
	},
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:shellscript"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^10.12.18",
		"tslint": "^5.11.1",
		"typescript": "^3.2.4",
		"vscode": "^1.1.27"
	}
}
